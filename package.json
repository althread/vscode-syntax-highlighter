{
  "name": "althread-language",
  "displayName": "Althread Language Support",
  "description": "Syntax highlighting for the Althread programming language",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
  "languages": [{
    "id": "althread",
    "aliases": ["Althread", "althread"],
    "extensions": [".alt"],
    "configuration": "./language-configuration.json",
    "icon": {
      "light": "./light_logo.png",
      "dark": "./light_logo.png"
    }
  }],
  "grammars": [{
    "language": "althread",
    "scopeName": "source.althread",
    "path": "./syntaxes/althread.tmLanguage.json"
  }]
},
  "repository": {
    "type": "git",
    "url": "https://github.com/althread/vscode-syntax-highlighter.git"
  }
}